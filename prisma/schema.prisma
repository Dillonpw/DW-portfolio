generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("POSTGRES_URL")
}

model Projects {
  id    Int     @id @default(autoincrement())
  key   String
  title  String
  image String
  mobileImage   String?
  liveLink  String?
  githubLink String
  tags String[]
  about   String
  Challenge String
}